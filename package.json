{
  "name": "dice",
  "version": "0.1.0",
  "type": "module",
  "packageManager": "yarn@4.9.4",
  "scripts": {
    "check": "tsc --noEmit",
    "build": "tsup",
    "types": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "test": "vitest run ",
    "test:watch": "vitest watch",
    "prepublishOnly": "yarn check && yarn test && yarn build",
    "example": "tsx examples/run.ts"
  },
  "imports": {
    "#/*": "./src/*"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@types/node": "^24.3.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "@vitest/coverage-v8": "^3.2.4"
  }
}
